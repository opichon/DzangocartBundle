{
    "name": "dzangocart/dzangocart-bundle",
    "description": "Symfony 2 bundle for dzangocart",
    "authors": [
        {
            "name": "Olivier Pichon",
            "email": "ofpichon@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "keywords": ["dzangocart", "symfony", "bundle"],
    "license": "MIT",
    "require": {
        "symfony/framework-bundle": "~2.0",
        "dzangocart/client": "0.2.*@dev",
        "goc/locale-bundle": "dev-master",
        "uam/datatables-bundle": "*"
    },
    "autoload": {
        "psr-0": {
            "Dzangocart\\Bundle\\DzangocartBundle": ""
        }
    },
    "target-dir": "Dzangocart/Bundle/DzangocartBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
