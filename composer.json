{
    "name": "dzangocart/dzangocart-bundle",
    "description": "Symfony 2 bundle for dzangocart",
    "type": "symfony-bundle",
    "keywords": ["dzangocart", "symfony", "bundle"],
    "license": "MIT",
    "require": {
        "symfony/symfony": ">=2.1,<2.3-dev",
        "dzangocart/client": "1.0.*@dev",
        "uam/datatables-bundle": "dev-master@dev"
    },
    "autoload": {
        "psr-0": {
            "Dzangocart\\Bundle\\DzangocartBundle": ""
        }
    },
    "target-dir": "Dzangocart/Bundle/DzangocartBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}

