    'bundles/dzangocart/vendor/momentjs/min/moment-with-langs.js'
    'bundles/dzangocart/vendor/bootstrap-daterangepicker/daterangepicker.js'
    'bundles/dzangocart/js/payment/payments.js'
    'bundles/dzangocart/js/ajax.js'
%}
