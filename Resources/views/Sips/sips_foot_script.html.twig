	'bundles/dzangocart/vendor/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js'
	'bundles/dzangocart/vendor/momentjs/min/moment-with-langs.js'
	'bundles/dzangocart/vendor/bootstrap-daterangepicker/daterangepicker.js'
	'bundles/dzangocart/js/ajax.js'
	'bundles/dzangocart/js/sips/sips.js'
%}
	<script src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>
dzangocart.sips = {
    datatables: {
        ajax: {
            url: "{{ path('dzangocart_sips_list') }}"
        }
