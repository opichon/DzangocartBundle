{% trans_default_domain "dzangocart" %}

{% spaceless %}
{
    "draw": "{{ app.request.query.all.draw }}",
    "recordsTotal": 0,
    "recordsFiltered": 0,
    "data" : [
        {% for s in list %}
            {
                "DT_RowId": "{{ 'u_' ~ s.id }}",
                "DT_RowClass": "{{ 'sips' }}",
                "0": "",
                "1": "{{ s.payment_date }}",
                "2": "{{ s.payment_time }}",
                "3": "{{ s.order_id }}",
                "4": "{{ s.amount }}",
                "5": "{{ s.currency_code }}",
                "6": "{{ s.payment_means }}",
                "7": "{{ s.card_number }}",
                "8": "{{ s.transaction_id }}",
                "9": "{{ s.response_code }}",
                "10": "{{ s.bank_response_code }}",
                "11": "{{ s.cvv_response_code }}",
                "12": ""
            }{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}
{% endspaceless %}
