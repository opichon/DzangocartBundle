{% trans_default_domain "dzangocart" %}

<div class="dzangocart sips">
	{% block filters %}
	{% include "DzangocartBundle:Sips:filters.html.twig" with { form: form } %}
	{% endblock %}
	<table class="table table-condensed">
		<thead>
			<tr>
				<th colspan="9"></th>
				<th colspan="3" class="center">{% trans %}dzangocart.sips.response_code.title{% endtrans %}</th>
				<th class="actions"></th>
			</tr>
			<tr>
				<th class="check"><input type="checkbox" id="checkall" /></th>
				<th class="date">{% trans %}dzangocart.sips.date{% endtrans %}</th>
				<th class="time">{% trans %}dzangocart.sips.time{% endtrans %}</th>
				<th class="order_id">{% trans %}dzangocart.sips.order_id{% endtrans %}</th>
				<th class="amount">{% trans %}dzangocart.sips.amount{% endtrans %}</th>
				<th class="currency">{% trans %}dzangocart.sips.currency{% endtrans %}</th>
				<th class="payment_means">{% trans %}dzangocart.sips.payment_means{% endtrans %}</th>
				<th class="card_number">{% trans %}dzangocart.sips.card_number{% endtrans %}</th>
				<th class="transaction_id">{% trans %}dzangocart.sips.transaction_id{% endtrans %}</th>
				<th class="code">{% trans %}dzangocart.sips.response_code.code{% endtrans %}</th>
				<th class="bank">{% trans %}dzangocart.sips.response_code.bank{% endtrans %}</th>
				<th class="cvv">{% trans %}dzangocart.sips.response_code.cvv{% endtrans %}</th>
				<th class="actions">{% trans %}dzangocart.sips.actions.title{% endtrans %}</th>
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
</div>