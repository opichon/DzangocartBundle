{% trans_default_domain "dzangocart" %}

{% spaceless %}
{
	"sEcho": "{{ app.request.query.all.sEcho }}",
	"iTotalRecords": {{ total }},
	"iTotalDisplayRecords": {{ totalDisplay }},
	"aaData" : [
		{% for order in list %}
			{
				"DT_RowId": "{{ 'u_' ~ order.getOrderId }}",
				"DT_RowClass": "{{ 'user' }}",
				"id": {{ order.getOrderId }},
				"0": "",
				"1": "{{ order.getDate|localizeddate('medium', 'short', app.request.locale, null, 'dd.MM.yyyy HH:mm') }}",
				"2": "{{ order.getOrderId }}",
				"3": "{{ order.getCustomer.getName }}",
				"4": "{{ order.getCurrency }}",
				"5": "",
				"6": "",
				"7": "{{ order.getAmount }}",
				"8": "{{ order.getAmountPaid }}",
				"9": "{{ order.affiliate }}",
				"10": "{{ order.isTest }}",
				"11": ""
			}{% if not loop.last %},{% endif %}
		{% endfor %}
	]
}
{% endspaceless %}