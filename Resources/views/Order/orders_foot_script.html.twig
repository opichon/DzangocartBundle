{% javascripts
	'bundles/dzangocart/vendor/datatables/media/js/jquery.dataTables.min.js'
	'bundles/dzangocart/vendor/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js'
	'bundles/dzangocart/vendor/typeahead.js/dist/typeahead.bundle.min.js'
	'bundles/dzangocart/vendor/momentjs/min/moment-with-langs.js'
	'bundles/dzangocart/vendor/bootstrap-daterangepicker/daterangepicker.js'
	'bundles/dzangocart/js/ajax.js'
	'bundles/dzangocart/js/order/orders.js'
%}
	<script src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>
dzangocart.orders = {
    datatables: {
        ajax: {
            url: "{{ path('dzangocart_orders_list') }}"
        }
    },
    date_format: "{{ config.date_format }}",
    daterangepicker: {
        format: "{{ config.date_format|upper }}"
    }
};
</script>
