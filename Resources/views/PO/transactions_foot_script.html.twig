{% javascripts
    'bundles/dzangocart/vendor/datatables/media/js/jquery.dataTables.min.js'
    'bundles/dzangocart/vendor/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js'
    'bundles/dzangocart/vendor/moment/min/moment-with-locales.min.js'
    'bundles/dzangocart/vendor/bootstrap-daterangepicker/daterangepicker.min.js'
    'bundles/dzangocart/js/ajax.js'
    'bundles/dzangocart/js/po/transactions.js'
%}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>
    dzangocart.po = {
        datatables: {
            ajax: {
                url: "{{ path('dzangocart_po_list') }}"
            }
        },
        daterangepicker: {
            format: "{{ config.date_format|upper }}",
            ranges: {% include "DzangocartBundle::date_ranges.html.twig" %}
        }
    };
